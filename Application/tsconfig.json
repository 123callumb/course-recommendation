{
  "compilerOptions": {
    "outDir": "_tsbin",
    "sourceMap": true,
    "noImplicitAny": true,
    "module": "commonjs",
    "target": "es6",
    "jsx": "react",
    "skipLibCheck": true, 
    "lib": [
      "es6",
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "esModuleInterop": true
  },
  "include": [
    "SPA"
  ],
  "exclude": [
    "node_modules"
  ]
}